{
	"compilerOptions": {
		"target": "es2016",
		"jsx": "react",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"resolveJsonModule": true,
		"outDir": "../../build/render",
		"removeComments": true,
		"paths": {
			"Components/*": ["./Components/*"],
			"Hooks/*": ["./Hooks/*"],
			"Utils/*": ["./Utils/*"]
		}
	},
	"include": ["./**/*"]
}
